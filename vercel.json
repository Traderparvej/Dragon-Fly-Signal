{
  "version": 2,
  "functions": {
    "api/login.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/signal", "dest": "/signal.html" },
    { "src": "/signal\\.html", "status": 301, "headers": { "Location": "/signal" } }
  ]
}
